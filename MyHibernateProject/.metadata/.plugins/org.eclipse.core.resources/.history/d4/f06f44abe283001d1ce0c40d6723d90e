package com.MainApplicaiton;
import javax.annotation.Generated;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

@Entity
public class Job_Applicant_Entity
{
	
	@Id 
	@GeneratedValue(strategy= GenerationType.AUTO)
	private int id;
	private String name;
	private int yop;
	private double result;
	private String email;
	
	public Job_Applicant_Entity()
	{
		System.out.println("entity classs executing");
	}
	
	
	public int getId() {
		return id;
	}


	public void setId(int id) {
		this.id = id;
	}


	public Job_Applicant_Entity(int id, String name, int yop, double result, String email) {
		super();
		this.id = id;
		this.name = name;
		this.yop = yop;
		this.result = result;
		this.email = email;
	}


	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getYop() {
		return yop;
	}
	public void setYop(int yop) {
		this.yop = yop;
	}
	public double getResult() {
		return result;
	}
	public void setResult(double result) {
		this.result = result;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	
	
	

}
